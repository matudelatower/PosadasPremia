<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.delatowebs.posadaspremia" >

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AdminActivity"
            android:label="@string/title_activity_admin"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.delatowebs.posadaspremia.MainActivity" />
        </activity>
        <activity
            android:name=".RegistroActivity"
            android:label="@string/title_activity_registro"
            android:parentActivityName=".ListadoRegistroActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.delatowebs.posadaspremia.ListadoRegistroActivity" />
        </activity>
        <activity
            android:name=".PersonaDetalle"
            android:label="@string/title_activity_persona_detalle"
            android:parentActivityName=".AdminActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.delatowebs.posadaspremia.AdminActivity" />
        </activity>
        <activity
            android:name=".ver_persona"
            android:label="@string/title_activity_ver_persona"
            android:parentActivityName=".AdminActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.delatowebs.posadaspremia.AdminActivity" />
        </activity>
        <activity
            android:name=".ListadoRegistroActivity"
            android:label="@string/title_activity_listado_registro"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.delatowebs.posadaspremia.MainActivity" />
        </activity>
        <activity
            android:name=".ConfigurarTabletActivity"
            android:label="@string/title_activity_configurar_tablet"
            android:parentActivityName=".AdminActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.delatowebs.posadaspremia.MainActivity" />
        </activity>
        <activity
            android:name=".ImportarPadronActivity"
            android:label="@string/title_activity_importar_padron"
            android:parentActivityName=".AdminActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.delatowebs.posadaspremia.MainActivity" />
        </activity>
        <activity
            android:name=".AndroidExplorer"
            android:label="@string/title_activity_importar_padron"
            android:parentActivityName=".ImportarPadronActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.delatowebs.posadaspremia.MainActivity" />
        </activity>
    </application>

</manifest>
